{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "encrypt:email": "node --env-file .env scripts/encrypt-email.ts"
  },
  "dependencies": {
    "@fontsource/italianno": "^5.2.8",
    "@fontsource/quattrocento": "^5.2.8",
    "intl-tel-input": "^25.12.4",
    "runed": "^0.35.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@iconify-json/game-icons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.72",
    "@iconify/tailwind4": "^1.1.0",
    "@internationalized/date": "^3.10.0",
    "@sveltejs/adapter-vercel": "^6.1.1",
    "@sveltejs/enhanced-img": "^0.8.4",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.10.0",
    "bits-ui": "^2.14.2",
    "clsx": "^2.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false,
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "tailwindStylesheet": "./src/app.css"
  }
}
