{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "encrypt:email": "node --env-file .env scripts/encrypt-email.ts"
  },
  "dependencies": {
    "@fontsource/italianno": "^5.2.8",
    "@fontsource/quattrocento": "^5.2.8",
    "intl-tel-input": "^25.15.0",
    "runed": "^0.37.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@iconify-json/game-icons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.83",
    "@iconify/tailwind4": "^1.2.0",
    "@internationalized/date": "^3.10.1",
    "@sveltejs/adapter-vercel": "^6.3.0",
    "@sveltejs/enhanced-img": "^0.9.2",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.5",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false,
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "tailwindStylesheet": "./src/app.css"
  }
}
